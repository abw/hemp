-- test '&&' when both true
[% 5 < 6 && 6 < 8 %]
-- expect
True

-- test '&&' when first true
[% 5 < 6 && 8 < 5 %]
-- expect
False

-- test '&&' when second true
[% 5 < 4 && 5 < 8 %]
-- expect
False

-- test '&&' when both false
[% 6 < 5 && 8 < 5 %]
-- expect
False

-- test '||' when both true
[% 5 < 6 || 6 < 8 %]
-- expect
True

-- test '||' when first true
[% 5 < 6 || 8 < 5 %]
-- expect
True

-- test '||' when second true
[% 6 < 5 || 1 < 2 %]
-- expect
True

-- test '||' when both false
[% 6 < 5 || 8 < 5 %]
-- expect
False

-- test not when false
[% not 5 < 6 %]
-- expect
False

-- test not when true
[% not 6 < 5 %]
-- expect
True

-- test and when both true
[% 5 < 6 and 6 < 8 %]
-- expect
True

-- test and when first true
[% 5 < 6 and 8 < 5 %]
-- expect
False

-- test and when second true
[% 5 < 4 and 5 < 8 %]
-- expect
False

-- test and when both false
[% 6 < 5 and 8 < 5 %]
-- expect
False

-- test or when both true
[% 5 < 6 or 6 < 8 %]
-- expect
True

-- test or when first true
[% 5 < 6 or 8 < 5 %]
-- expect
True

-- test or when second true
[% 6 < 5 or 1 < 2 %]
-- expect
True

-- test or when both false
[% 6 < 5 or 8 < 5 %]
-- expect
False
